  "description": "The tech talent hire app",
  "main": "index.js",
  "scripts": {
    "prestart": "npm run init:cert",
    "init:cert": "run-script-os",
    "init:cert:win32": "@powershell -NoProfile -ExecutionPolicy Unrestricted -Command ./webpack/host-certificates/init-cert.ps1",
    "init:cert:default": "echo 'Please, refer to setup guide to see how to configure environment'",
    "start": "cross-env NODE_ENV=development webpack serve --mode development --config webpack/app.config.js",
    "build:prod": "cross-env NODE_ENV=production webpack --config webpack/app.config.js",
    "test": "jest",
    "test:dev": "jest --watch"
  },
